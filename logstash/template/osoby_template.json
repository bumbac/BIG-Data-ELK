{
  "index_patterns": "osoby",
  "settings": {
    "index.refresh_interval": "5s"
  },
  "mappings": {
    "doc": {
      "dynamic_templates": [
        {
          "string_fields": {
            "mapping": {
              "type": "text",
              "omit_norms": true,
              "fields": {
                "keyword": {
                  "type": "keyword",
                  "ignore_above": 256
                }
              }
            },
            "match_mapping_type": "string",
            "match": "*"
          }
        }
      ],
      "properties": {
        "datum": {
          "type": "date", "format": "yyyy-MM-dd"
        },
        "vek": {
          "type": "integer"
        },
        "pohlavi": {
          "type": "keyword"
        },
        "kraj_nuts_kod": {
          "type": "keyword"
        },
        "okres_lau_kod": {
          "type": "keyword"
        },
        "nakaza_v_zahranici": {
          "type": "keyword"
        },
        "nakaza_zeme_csu_kod": {
          "type": "keyword",
          "ignore_malformed": true
        }
      }
    }
  },
  "aliases": {}
}
